          <File RecordLimit="" FileFormat="17">aka:5fda36ca022c061b0852e8f1|||select AP.AP_SUPPLIERS.SEGMENT1 "Supplier Number",
	AP.AP_SUPPLIERS.VENDOR_NAME "Supplier Name",
	PO_VENDOR_SITES.VENDOR_SITE_CODE "Sites Code",
	AP.AP_SUPPLIERS.VENDOR_TYPE_LOOKUP_CODE "Supplier Type",
	PO_VENDOR_SITES.ADDRESS_LINE1 || ' ' || PO_VENDOR_SITES.ADDRESS_LINE2 || ' ' || PO_VENDOR_SITES.ADDRESS_LINE3 "Address",
	PO_VENDOR_SITES.CITY "City",
	PO_VENDOR_SITES.STATE "State",
	PO_VENDOR_SITES.COUNTRY "Country",
	PO_VENDOR_SITES.ZIP "Postal Code",
	HR.HR_ALL_ORGANIZATION_UNITS.NAME "Operating Unit",
	'(' || PO_VENDOR_SITES.AREA_CODE || ') ' || PO_VENDOR_SITES.PHONE as "Phone Number",
	AP.AP_TERMS_TL.NAME "Supplier Terms",
	SITE_TERMS.NAME "Site Terms",
	PO_VENDOR_SITES.INACTIVE_DATE "Sites Inactive Date",
	'(' || PO_VENDOR_SITES.FAX_AREA_CODE || ')' || PO_VENDOR_SITES.FAX as "Fax",
	Decode(AP.AP_SUPPLIERS.INSPECTION_REQUIRED_FLAG || AP.AP_SUPPLIERS.RECEIPT_REQUIRED_FLAG, 'NN', '2-way', 'NY', '3-way', 'YY', '4-way', 'NOT SET') "Supplier Match Level",
	AP.AP_SUPPLIERS.ONE_TIME_FLAG "Supplier One Time Flag (Y/N)",
	PO_VENDOR_SITES.PRIMARY_PAY_SITE_FLAG "Primary Pay Site (Y/N)",
	PO_VENDOR_SITES.PURCHASING_SITE_FLAG "Purchasing Site (Y/N)",
	PO_VENDOR_SITES.PAY_SITE_FLAG "Payment Site (Y/N)",
	AP.AP_SUPPLIERS.NUM_1099 "Supplier Tax ID",
	POV_SOB.NAME "Supplier Set of Book",
	AP.AP_SUPPLIERS.CREATION_DATE "Supplier Creation Date",
	AP.AP_SUPPLIERS.END_DATE_ACTIVE "Supplier Inactive Date",
	NVL(FND_USER_PO_V1.DESCRIPTION, FND_USER_PO_V1.USER_NAME) "Sites Created By",
	NVL(FND_USER_PO_V2.DESCRIPTION, FND_USER_PO_V2.USER_NAME) "Sites Last Updated By",
	PO_VENDOR_SITES.CREATION_DATE "Sites Create Date",
	PO_VENDOR_SITES.HOLD_ALL_PAYMENTS_FLAG "Sites Hold All Payments Flag",
	PO_VENDOR_SITES.HOLD_REASON "Sites Hold Reason",
	SITES_PM.PAYMENT_METHOD_CODE "Sites Payment Method Code",
	(
	select IEPPM.PAYMENT_METHOD_CODE 
	from IBY.IBY_EXTERNAL_PAYEES_ALL IEP, IBY.IBY_EXT_PARTY_PMT_MTHDS IEPPM 
	where IEPPM.EXT_PMT_PARTY_ID = IEP.EXT_PAYEE_ID 
		and IEP.PAYMENT_FUNCTION = IEPPM.PAYMENT_FUNCTION 
		and IEP.PAYEE_PARTY_ID = AP.AP_SUPPLIERS.PARTY_ID 
		and IEP.PAYMENT_FUNCTION = 'PAYABLES_DISB' 
		and IEPPM.PRIMARY_FLAG = 'Y' 
		and NVL(IEPPM.INACTIVE_DATE, SysDate) &gt;= SysDate 
		and IEP.SUPPLIER_SITE_ID is NULL 
		and IEP.PARTY_SITE_ID is Null
	) "Supplier Payment Method Code",
	IBY_SITES.REMIT_ADVICE_EMAIL "Sites Email Address",
	PO_VENDOR_SITES.EMAIL_ADDRESS "Remittance Email",
	AP.AP_SUPPLIERS.VENDOR_NAME_ALT "Supplier Alternate Name",
	AP.AP_SUPPLIERS.MINORITY_GROUP_LOOKUP_CODE "Minority Group Flag",
	AP.AP_SUPPLIERS.WOMEN_OWNED_FLAG "Women Owned Flag (Y/N)",
	AP.AP_SUPPLIERS.SMALL_BUSINESS_FLAG "Small Owned Flag (Y/N)",
	POV_HSOB.NAME "Supplier FOB",
	PO_VENDOR_CONTACTS.LAST_NAME "Last Name",
	PO_VENDOR_CONTACTS.FIRST_NAME "First Name",
	PO_VENDOR_SITES.PAYMENT_CURRENCY_CODE,
	PO_VENDOR_SITES.VENDOR_SITE_ID,
	AP.AP_SUPPLIERS.FEDERAL_REPORTABLE_FLAG as "Tax Reportable Flag",
	PO_VENDOR_SITES.EXCLUSIVE_PAYMENT_FLAG as "Site Pay Only Flag",
	AP.AP_SUPPLIERS.EXCLUSIVE_PAYMENT_FLAG as "Supplier Pay Only Flag",
	PO_VENDOR_SITES.PAYMENT_METHOD_LOOKUP_CODE as PO_SITE_PAYMENT_CODE,
	SITES_PM.PRIMARY_FLAG 
from AP.AP_SUPPLIERS, HR.HR_ALL_ORGANIZATION_UNITS, AP.AP_SUPPLIER_SITES_ALL PO_VENDOR_SITES, AP.AP_TERMS_TL, AP.AP_TERMS_TL SITE_TERMS, APPLSYS.FND_USER FND_USER_PO_V1, APPLSYS.FND_USER FND_USER_PO_V2, GL.GL_LEDGERS POV_SOB, GL.GL_LEDGERS POV_HSOB, HR.HR_ORGANIZATION_INFORMATION HOI, IBY.IBY_EXTERNAL_PAYEES_ALL IBY_SITES, IBY.IBY_EXT_PARTY_PMT_MTHDS SITES_PM, AP.AP_SUPPLIER_CONTACTS PO_VENDOR_CONTACTS 
where (PO_VENDOR_CONTACTS.VENDOR_SITE_ID(+) = PO_VENDOR_SITES.VENDOR_SITE_ID) 
	and (AP.AP_TERMS_TL.TERM_ID(+) = AP.AP_SUPPLIERS.TERMS_ID) 
	and (HR.HR_ALL_ORGANIZATION_UNITS.ORGANIZATION_ID = HOI.ORGANIZATION_ID) 
	and SITE_TERMS.TERM_ID(+) = PO_VENDOR_SITES.TERMS_ID 
	and (FND_USER_PO_V1.USER_ID(+) = PO_VENDOR_SITES.CREATED_BY) 
	and (FND_USER_PO_V2.USER_ID(+) = PO_VENDOR_SITES.LAST_UPDATED_BY) 
	and (HOI.ORG_INFORMATION3 = POV_SOB.LEDGER_ID) 
	and (HOI.ORGANIZATION_ID = PO_VENDOR_SITES.ORG_ID) 
	and (AP.AP_SUPPLIERS.VENDOR_ID = PO_VENDOR_SITES.VENDOR_ID) 
	and IBY_SITES.SUPPLIER_SITE_ID(+) = PO_VENDOR_SITES.VENDOR_SITE_ID 
	and IBY_SITES.ORG_ID(+) = PO_VENDOR_SITES.ORG_ID 
	and SITES_PM.EXT_PMT_PARTY_ID(+) = IBY_SITES.EXT_PAYEE_ID 
	and SITES_PM.PAYMENT_FUNCTION(+) = IBY_SITES.PAYMENT_FUNCTION 
	and (AP.AP_SUPPLIERS.SET_OF_BOOKS_ID = POV_HSOB.LEDGER_ID(+)) 
	and AP.AP_SUPPLIERS.VENDOR_TYPE_LOOKUP_CODE not in ('EMPLOYEE', 'SBS EMPLOYEE', 'SBS SUPPLIER') 
	and (PO_VENDOR_SITES.INACTIVE_DATE &gt;= Trunc(SysDate) or PO_VENDOR_SITES.INACTIVE_DATE is Null) 
	and SITES_PM.PRIMARY_FLAG = 'Y' 
	and HOI.ORG_INFORMATION_CONTEXT = 'Operating Unit Information' 
	and NVL(SITES_PM.INACTIVE_DATE(+), SysDate) &gt;= SysDate</File>
