          <File RecordLimit="" FileFormat="17">aka:5f11ec5cf88a2f1cc8948256|||select AP_CHECKS_ALL.PAYMENT_METHOD_CODE PAYMENT_METHOD_CODE,
	Sum(NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT)) as SUM_INV_HDR_AMNT,
	case 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '82' then 'United States' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '142' then 'United Kingdom' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '113' then 'Singapore' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID in ('129', '132', '145') then 'India' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '139' then 'Canada' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '186' then 'Australia' 
			else HR_ALL_ORGANIZATION_UNITS2.NAME 
		end as INV_HDR_OPERATING_UNIT,
	PO.PO_HEADERS_ALL.SEGMENT1 PO_HEADER_NUMBER,
	AP_INV_VENDORS.SEGMENT1 SUPPLIER_NUMBER,
	NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) INV_HDR_AMNT,
	AP_TERMS_TL_INV.NAME INV_HDR_TERMS,
	AP.AP_INVOICES_ALL.SOURCE INV_SOURCE,
	AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE INV_CURRENCY_CODE,
	AP_INV_VENDORS.VENDOR_TYPE_LOOKUP_CODE VENDOR_TYPE_LOOKUP_CODE,
	AP_INV_VENDORS.VENDOR_NAME SUPPLIER_NAME,
	AP.AP_INVOICES_ALL.INVOICE_NUM INVOICE_NUMBER,
	AP.AP_INVOICES_ALL.CREATION_DATE INV_CREATION_DATE,
	AP.AP_INVOICES_ALL.INVOICE_DATE INV_DATE,
	AP.AP_PAYMENT_SCHEDULES_ALL.DUE_DATE INV_HDR_DUE_DATE,
	FND_USER_AP.USER_NAME INV_HDR_CRT_BY,
	AP.AP_INVOICES_ALL.AMOUNT_PAID INV_HDR_PAID_AMT,
	AP_CHECKS_ALL.CHECK_DATE INV_PMT_DATE,
	AP_CHECKS_ALL.CHECK_NUMBER INV_PMT_NUMBER,
	AP.AP_INVOICES_ALL.INVOICE_RECEIVED_DATE INV_RECEIVED_DATE,
	case 
			when GL_COMB_AP.SEGMENT1 = '250' then 'India Tech' 
			when GL_COMB_AP.SEGMENT1 = '255' then 'India Soft' 
			when GL_COMB_AP.SEGMENT1 in ('256', '257') then 'India Dev' 
			when GL_COMB_AP.SEGMENT1 = '200' then 'Singapore' 
			when GL_COMB_AP.SEGMENT1 = '210' then 'United Kingdom' 
			when GL_COMB_AP.SEGMENT1 = '230' then 'Canada' 
			when GL_COMB_AP.SEGMENT1 Like '1%' then 'United States' 
			else INV_SOB.NAME 
		end INV_HDR_CTRY_NAME,
	AP.AP_INVOICES_ALL.DESCRIPTION INV_DESCRIPTION,
	AP.AP_INVOICES_ALL.PAYMENT_METHOD_CODE as INV_HDR_METHOD_CODE,
	AP.AP_TERMS_TL.NAME INV_TERM_NAME 
from AP.AP_CHECKS_ALL AP_CHECKS_ALL, AP.AP_INVOICES_ALL, HR.HR_ALL_ORGANIZATION_UNITS HR_ALL_ORGANIZATION_UNITS2, PO.PO_HEADERS_ALL, AP.AP_SUPPLIERS AP_INV_VENDORS, AP.AP_TERMS_TL AP_TERMS_TL_INV, AP.AP_PAYMENT_SCHEDULES_ALL, APPLSYS.FND_USER FND_USER_AP, GL.GL_CODE_COMBINATIONS GL_COMB_AP, AP.AP_TERMS_TL, AP.AP_INVOICE_PAYMENTS_ALL, PO.PO_LINES_ALL, PO.PO_LINE_LOCATIONS_ALL, PO.PO_DISTRIBUTIONS_ALL, AP.AP_INVOICE_DISTRIBUTIONS_ALL, AP.AP_SUPPLIERS, GL.GL_LEDGERS INV_SOB 
where (PO.PO_DISTRIBUTIONS_ALL.LINE_LOCATION_ID = PO.PO_LINE_LOCATIONS_ALL.LINE_LOCATION_ID(+)) 
	and (PO.PO_LINE_LOCATIONS_ALL.PO_LINE_ID = PO.PO_LINES_ALL.PO_LINE_ID(+)) 
	and (PO.PO_LINES_ALL.PO_HEADER_ID = PO.PO_HEADERS_ALL.PO_HEADER_ID(+)) 
	and (AP.AP_INVOICE_DISTRIBUTIONS_ALL.PO_DISTRIBUTION_ID = PO.PO_DISTRIBUTIONS_ALL.PO_DISTRIBUTION_ID(+)) 
	and (AP.AP_INVOICES_ALL.INVOICE_ID = AP.AP_INVOICE_DISTRIBUTIONS_ALL.INVOICE_ID) 
	and (PO.PO_HEADERS_ALL.VENDOR_ID = AP.AP_SUPPLIERS.VENDOR_ID) 
	and (GL_COMB_AP.CODE_COMBINATION_ID = AP.AP_INVOICE_DISTRIBUTIONS_ALL.DIST_CODE_COMBINATION_ID) 
	and (AP.AP_PAYMENT_SCHEDULES_ALL.INVOICE_ID = AP.AP_INVOICES_ALL.INVOICE_ID) 
	and (FND_USER_AP.USER_ID = AP.AP_INVOICES_ALL.CREATED_BY) 
	and (AP.AP_TERMS_TL.TERM_ID(+) = AP.AP_SUPPLIERS.TERMS_ID) 
	and (AP_TERMS_TL_INV.TERM_ID(+) = AP.AP_INVOICES_ALL.TERMS_ID) 
	and (AP.AP_INVOICES_ALL.VENDOR_ID = AP_INV_VENDORS.VENDOR_ID) 
	and (AP.AP_INVOICE_PAYMENTS_ALL.INVOICE_ID(+) = AP.AP_INVOICES_ALL.INVOICE_ID) 
	and (AP.AP_INVOICES_ALL.SET_OF_BOOKS_ID = INV_SOB.LEDGER_ID) 
	and (AP_CHECKS_ALL.CHECK_ID(+) = AP.AP_INVOICE_PAYMENTS_ALL.CHECK_ID) 
	and (AP.AP_INVOICES_ALL.ORG_ID = HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID) 
	and AP_INV_VENDORS.VENDOR_TYPE_LOOKUP_CODE &lt;&gt; 'EMPLOYEE' 
	and AP.AP_INVOICES_ALL.CREATION_DATE Between '01-JUN-2021' 
	and '30-JUN-2021' 
	and PO.PO_DISTRIBUTIONS_ALL.GL_CANCELLED_DATE is NULL 
	and (AP.AP_INVOICE_PAYMENTS_ALL.REVERSAL_FLAG = 'N' or AP.AP_INVOICE_PAYMENTS_ALL.REVERSAL_FLAG is Null) 
group by AP_CHECKS_ALL.PAYMENT_METHOD_CODE, case 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '82' then 'United States' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '142' then 'United Kingdom' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '113' then 'Singapore' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID in ('129', '132', '145') then 'India' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '139' then 'Canada' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '186' then 'Australia' 
			else HR_ALL_ORGANIZATION_UNITS2.NAME 
		end, PO.PO_HEADERS_ALL.SEGMENT1, AP_INV_VENDORS.SEGMENT1, NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT), AP_TERMS_TL_INV.NAME, AP.AP_INVOICES_ALL.SOURCE, AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE, AP_INV_VENDORS.VENDOR_TYPE_LOOKUP_CODE, AP_INV_VENDORS.VENDOR_NAME, AP.AP_INVOICES_ALL.INVOICE_NUM, AP.AP_INVOICES_ALL.CREATION_DATE, AP.AP_INVOICES_ALL.INVOICE_DATE, AP.AP_PAYMENT_SCHEDULES_ALL.DUE_DATE, FND_USER_AP.USER_NAME, AP.AP_INVOICES_ALL.AMOUNT_PAID, AP_CHECKS_ALL.CHECK_DATE, AP_CHECKS_ALL.CHECK_NUMBER, AP.AP_INVOICES_ALL.INVOICE_RECEIVED_DATE, case 
			when GL_COMB_AP.SEGMENT1 = '250' then 'India Tech' 
			when GL_COMB_AP.SEGMENT1 = '255' then 'India Soft' 
			when GL_COMB_AP.SEGMENT1 in ('256', '257') then 'India Dev' 
			when GL_COMB_AP.SEGMENT1 = '200' then 'Singapore' 
			when GL_COMB_AP.SEGMENT1 = '210' then 'United Kingdom' 
			when GL_COMB_AP.SEGMENT1 = '230' then 'Canada' 
			when GL_COMB_AP.SEGMENT1 Like '1%' then 'United States' 
			else INV_SOB.NAME 
		end, AP.AP_INVOICES_ALL.DESCRIPTION, AP.AP_INVOICES_ALL.PAYMENT_METHOD_CODE, AP.AP_TERMS_TL.NAME</File>
