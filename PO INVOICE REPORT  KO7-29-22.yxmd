          <File RecordLimit="" FileFormat="17">aka:5fda36ca022c061b0852e8f1|||select distinct poh.SEGMENT1 PO_Number,
	pol.CREATION_DATE PO_Line_Creation_Date,
	pol.ITEM_DESCRIPTION PO_Description,
	polt.LINE_TYPE PO_Type,
	s.VENDOR_NAME Supplier_Name,
	inv.CREATION_DATE INVOICE_CREATION_DATE,
	inv.INVOICE_DATE Invoice_Date,
	inv.INVOICE_NUM Invoice_Number,
	inv.DESCRIPTION Invoice_Description,
	inv.INVOICE_CURRENCY_CODE Invoice_Currency,
	NVL(inv.INVOICE_AMOUNT, inv.BASE_AMOUNT) Invoice_Amount,
	case 
			when inv.INVOICE_CURRENCY_CODE = 'USD' then NVL(inv.INVOICE_AMOUNT, inv.BASE_AMOUNT) 
			else (NVL(inv.INVOICE_AMOUNT, inv.BASE_AMOUNT) * DAILY_CONV_RATE.CONVERSION_RATE) 
		end Invoice_Amount_USD,
	HR.HR_ALL_ORGANIZATION_UNITS.NAME as OU,
	pol.PO_LINE_ID as "PO LIne",
	pol.UNIT_PRICE as "PO Unit Price",
	pol.QUANTITY as "Po QTY",
	pol.AMOUNT as "PO AMOUNT",
	poh.CURRENCY_CODE as "PO CURRENCY",
	s.SEGMENT1,
	s.VENDOR_ID,
	inv.TOTAL_TAX_AMOUNT,
	inv.INVOICE_AMOUNT as INVOICE_AMOUNT1,
	AP.AP_INVOICE_LINES_ALL.RETAINED_AMOUNT as "INV LINE RETAINED",
	AP.AP_INVOICE_LINES_ALL.UNIT_PRICE as "INV LINE PRICE",
	AP.AP_INVOICE_LINES_ALL.QUANTITY_INVOICED as "INV LINE QTY",
	AP.AP_INVOICE_LINES_ALL.AMOUNT as "INV LINE AMOUNT",
	AP.AP_INVOICE_LINES_ALL.LINE_NUMBER as "INV LINE NUM",
	AP.AP_INVOICE_LINES_ALL.LINE_TYPE_LOOKUP_CODE,
	AP.AP_INVOICE_LINES_ALL.LINE_SOURCE,
	AP.AP_INVOICE_LINES_ALL.INVOICE_ID 
from PO.PO_HEADERS_ALL poh, PO.PO_LINES_ALL pol, PO.PO_LINE_TYPES_TL polt, AP.AP_SUPPLIERS s, AP.AP_INVOICES_ALL inv, AP.AP_INVOICE_DISTRIBUTIONS_ALL invd, PO.PO_LINE_LOCATIONS_ALL pll, PO.PO_DISTRIBUTIONS_ALL pod, AP.AP_INVOICE_PAYMENTS_ALL invp, 
	(
	select gl.gl_daily_rates.FROM_CURRENCY,
		gl.gl_daily_rates.CONVERSION_RATE,
		gl.gl_daily_rates.CONVERSION_DATE,
		gl.gl_daily_rates.CONVERSION_TYPE 
	from gl.gl_daily_rates 
	where gl.gl_daily_rates.CONVERSION_TYPE = 'Corporate' 
		and gl.gl_daily_rates.TO_CURRENCY = 'USD'
	) DAILY_CONV_RATE, HR.HR_ALL_ORGANIZATION_UNITS, AP.AP_INVOICE_LINES_ALL 
where (pod.LINE_LOCATION_ID = pll.LINE_LOCATION_ID(+)) 
	and (DAILY_CONV_RATE.FROM_CURRENCY(+) = inv.INVOICE_CURRENCY_CODE) 
	and (DAILY_CONV_RATE.CONVERSION_DATE(+) = Trunc(inv.CREATION_DATE)) 
	and (pll.PO_LINE_ID = pol.PO_LINE_ID(+)) 
	and (pol.PO_HEADER_ID = poh.PO_HEADER_ID(+)) 
	and (invd.PO_DISTRIBUTION_ID = pod.PO_DISTRIBUTION_ID(+)) 
	and (inv.INVOICE_ID = invd.INVOICE_ID) 
	and (polt.LINE_TYPE_ID = pol.LINE_TYPE_ID) 
	and (inv.VENDOR_ID = s.VENDOR_ID) 
	and (invp.INVOICE_ID = inv.INVOICE_ID) 
	and pol.ORG_ID = HR.HR_ALL_ORGANIZATION_UNITS.ORGANIZATION_ID 
	and inv.INVOICE_ID = AP.AP_INVOICE_LINES_ALL.INVOICE_ID(+) 
	and pol.CREATION_DATE Between '20-JUL-2022' 
	and '30-JUL-2022' 
	and AP.AP_INVOICE_LINES_ALL.LINE_TYPE_LOOKUP_CODE = 'ITEM' 
	and (invp.REVERSAL_FLAG = 'N' or invp.REVERSAL_FLAG is Null)</File>
