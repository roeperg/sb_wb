	AP_CHECKS_ALL.CHECK_NUMBER as INV_PMT_NUMBER,
	AP.AP_INVOICES_ALL.INVOICE_RECEIVED_DATE as INV_HDR_REC_DATE,
	Sum(NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT)) as SUM_INV_HDR_AMNT,
	HR_ALL_ORGANIZATION_UNITS2.NAME INV_HDR_OPERATING_UNIT,
	AP.AP_INVOICES_ALL.DESCRIPTION as INV_HDR_DESC,
	AP.AP_INVOICES_ALL.PAYMENT_METHOD_CODE as INV_HDR_PMT_TYPE,
	AP.AP_INVOICES_ALL.GL_DATE as INV_HDR_GL_DATE,
	AP_VENDOR_SITES.VENDOR_SITE_CODE VENDOR_SITE_CODE,
	APSITE_COMB.SEGMENT1 as SITES_COMPANY_CODE,
	AP_VENDOR_SITES.CITY as VENDOR_CITY,
	AP_VENDOR_SITES.STATE as VENDOR_STATE,
	AP_VENDOR_SITES.COUNTRY as VENDOR_COUNTRY,
	GL_COMB_AP.SEGMENT1,
	GL_COMB_AP.SEGMENT2,
	GL_COMB_AP.SEGMENT3,
	GL_COMB_AP.SEGMENT4,
	GL_COMB_AP.CODE_COMBINATION_ID 
from AP.AP_CHECKS_ALL AP_CHECKS_ALL, AP.AP_PAYMENT_SCHEDULES_ALL, AP.AP_SUPPLIERS AP_INV_VENDORS, AP.AP_INVOICES_ALL, AP.AP_TERMS_TL AP_TERMS_TL_INV, APPLSYS.FND_USER FND_USER_AP, GL.GL_CODE_COMBINATIONS GL_COMB_AP, HR.HR_ALL_ORGANIZATION_UNITS HR_ALL_ORGANIZATION_UNITS2, AP.AP_SUPPLIER_SITES_ALL AP_VENDOR_SITES, GL.GL_CODE_COMBINATIONS APSITE_COMB, PO.PO_HEADERS_ALL, AP.AP_INVOICE_PAYMENTS_ALL, AP.AP_INVOICE_DISTRIBUTIONS_ALL, PO.PO_LINES_ALL, PO.PO_LINE_LOCATIONS_ALL, PO.PO_DISTRIBUTIONS_ALL, GL.GL_LEDGERS INV_SOB 
where (PO.PO_DISTRIBUTIONS_ALL.LINE_LOCATION_ID = PO.PO_LINE_LOCATIONS_ALL.LINE_LOCATION_ID(+)) 
	and (PO.PO_LINE_LOCATIONS_ALL.PO_LINE_ID = PO.PO_LINES_ALL.PO_LINE_ID(+)) 
	and (PO.PO_LINES_ALL.PO_HEADER_ID = PO.PO_HEADERS_ALL.PO_HEADER_ID(+)) 
	and (AP.AP_INVOICE_DISTRIBUTIONS_ALL.PO_DISTRIBUTION_ID = PO.PO_DISTRIBUTIONS_ALL.PO_DISTRIBUTION_ID(+)) 
	and (AP.AP_INVOICES_ALL.INVOICE_ID = AP.AP_INVOICE_DISTRIBUTIONS_ALL.INVOICE_ID) 
	and (GL_COMB_AP.CODE_COMBINATION_ID = AP.AP_INVOICE_DISTRIBUTIONS_ALL.DIST_CODE_COMBINATION_ID) 
	and (AP.AP_PAYMENT_SCHEDULES_ALL.INVOICE_ID = AP.AP_INVOICES_ALL.INVOICE_ID) 
	and (FND_USER_AP.USER_ID = AP.AP_INVOICES_ALL.CREATED_BY) 
	and (AP_TERMS_TL_INV.TERM_ID(+) = AP.AP_INVOICES_ALL.TERMS_ID) 
	and (AP.AP_INVOICES_ALL.VENDOR_ID = AP_INV_VENDORS.VENDOR_ID) 
	and (AP.AP_INVOICE_PAYMENTS_ALL.INVOICE_ID(+) = AP.AP_INVOICES_ALL.INVOICE_ID) 
	and (AP.AP_INVOICES_ALL.SET_OF_BOOKS_ID = INV_SOB.LEDGER_ID) 
	and (AP_CHECKS_ALL.CHECK_ID(+) = AP.AP_INVOICE_PAYMENTS_ALL.CHECK_ID) 
	and (AP.AP_INVOICES_ALL.VENDOR_SITE_ID = AP_VENDOR_SITES.VENDOR_SITE_ID(+)) 
	and (APSITE_COMB.CODE_COMBINATION_ID = AP_VENDOR_SITES.ACCTS_PAY_CODE_COMBINATION_ID) 
	and (AP.AP_INVOICES_ALL.ORG_ID = HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID) 
	and AP_CHECKS_ALL.CHECK_DATE Between '01-NOV-2021' 
	and '10-FEB-2023' 
	and GL_COMB_AP.SEGMENT2 = '087' 
	and PO.PO_DISTRIBUTIONS_ALL.GL_CANCELLED_DATE is NULL 
	and (AP.AP_INVOICE_PAYMENTS_ALL.REVERSAL_FLAG = 'N' or AP.AP_INVOICE_PAYMENTS_ALL.REVERSAL_FLAG is Null) 
group by AP_CHECKS_ALL.PAYMENT_METHOD_CODE, AP.AP_PAYMENT_SCHEDULES_ALL.DISCOUNT_DATE, AP_INV_VENDORS.SEGMENT1, AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT, NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT), AP_TERMS_TL_INV.NAME, AP.AP_INVOICES_ALL.SOURCE, AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE, AP_INV_VENDORS.VENDOR_TYPE_LOOKUP_CODE, AP_INV_VENDORS.VENDOR_NAME, AP.AP_INVOICES_ALL.INVOICE_NUM, AP.AP_INVOICES_ALL.CREATION_DATE, AP.AP_INVOICES_ALL.INVOICE_DATE, AP.AP_PAYMENT_SCHEDULES_ALL.DUE_DATE, FND_USER_AP.USER_NAME, AP.AP_INVOICES_ALL.AMOUNT_PAID, AP_CHECKS_ALL.CHECK_DATE, AP_CHECKS_ALL.CHECK_NUMBER, AP.AP_INVOICES_ALL.INVOICE_RECEIVED_DATE, HR_ALL_ORGANIZATION_UNITS2.NAME, AP.AP_INVOICES_ALL.DESCRIPTION, AP.AP_INVOICES_ALL.PAYMENT_METHOD_CODE, AP.AP_INVOICES_ALL.GL_DATE, AP_VENDOR_SITES.VENDOR_SITE_CODE, APSITE_COMB.SEGMENT1, AP_VENDOR_SITES.CITY, AP_VENDOR_SITES.STATE, AP_VENDOR_SITES.COUNTRY, GL_COMB_AP.SEGMENT1, GL_COMB_AP.SEGMENT2, GL_COMB_AP.SEGMENT3, GL_COMB_AP.SEGMENT4, GL_COMB_AP.CODE_COMBINATION_ID</File>
