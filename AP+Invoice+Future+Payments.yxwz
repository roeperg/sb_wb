          <File RecordLimit="" FileFormat="17">aka:5fda36ca022c061b0852e8f1|||select AP.AP_PAYMENT_SCHEDULES_ALL.DUE_DATE "AP Invoice Hdr Due Date",
	AP_INV_VENDORS.VENDOR_NAME "AP Supplier Name",
	AP_VENDOR_SITES.HOLD_ALL_PAYMENTS_FLAG "Sites Hold All Payments Flag",
	AP.AP_INVOICES_ALL.SOURCE "AP Invoice Hdr Source",
	AP.AP_INVOICES_ALL.INVOICE_NUM "AP Invoice Hdr Num",
	AP.AP_INVOICES_ALL.INVOICE_DATE "AP Invoice Hdr Dt",
	AP.AP_INVOICES_ALL.CREATION_DATE "AP Invoice Hdr Create Dt",
	AP.AP_PAYMENT_SCHEDULES_ALL.DISCOUNT_DATE "AP Invoice Hdr Discount Due Dt",
	Decode(AHA.INVOICE_ID, Null, 'N', 'Y') "AP Invoice Hdr Hold (Y/N)",
	AP_CHECKS_ALL.CHECK_NUMBER "AP Payment Num",
	AP.AP_INVOICES_ALL.PAYMENT_STATUS_FLAG "AP Invoice Hdr Pay Status",
	AP_CHECKS_ALL.CHECK_DATE "AP Payment Dt",
	AP.AP_INVOICES_ALL.PAYMENT_METHOD_CODE "AP Invoice Hdr Pay Type",
	AP.AP_INVOICES_ALL.INVOICE_TYPE_LOOKUP_CODE "AP Invoice Hdr Type",
	INV_SOB.NAME "AP Invoice Hdr SoB Name",
	case 
			when AP.AP_INVOICES_ALL.PAYMENT_STATUS_FLAG = 'P' then AP.AP_PAYMENT_SCHEDULES_ALL.AMOUNT_REMAINING 
			else Decode(AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE, 'USD', AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) 
		end as "AP Invoice Hdr Amt",
	AP.AP_PAYMENT_SCHEDULES_ALL.DISCOUNT_AMOUNT_AVAILABLE "AP Invoice Hdr Dis Amt" 
from 
AP.AP_PAYMENT_SCHEDULES_ALL, 
AP.AP_SUPPLIERS AP_INV_VENDORS, 
AP.AP_SUPPLIER_SITES_ALL AP_VENDOR_SITES, 
AP.AP_INVOICES_ALL, 
	(
	select distinct AP.AP_HOLDS_ALL.INVOICE_ID,
		AP.AP_HOLDS_ALL.RELEASE_LOOKUP_CODE 
	from AP.AP_HOLDS_ALL
	) AHA, AP.AP_CHECKS_ALL AP_CHECKS_ALL, AP.AP_INVOICE_PAYMENTS_ALL, GL.GL_LEDGERS INV_SOB 
where (AP.AP_PAYMENT_SCHEDULES_ALL.INVOICE_ID = AP.AP_INVOICES_ALL.INVOICE_ID) 
	and (AHA.INVOICE_ID(+) = AP.AP_INVOICES_ALL.INVOICE_ID) 
	and (AP.AP_INVOICES_ALL.VENDOR_ID = AP_INV_VENDORS.VENDOR_ID) 
	and (AP.AP_INVOICE_PAYMENTS_ALL.INVOICE_ID(+) = AP.AP_INVOICES_ALL.INVOICE_ID) 
	and (AP.AP_INVOICES_ALL.SET_OF_BOOKS_ID = INV_SOB.LEDGER_ID) 
	and (AP_CHECKS_ALL.CHECK_ID(+) = AP.AP_INVOICE_PAYMENTS_ALL.CHECK_ID) 
	and (AP.AP_INVOICES_ALL.VENDOR_SITE_ID = AP_VENDOR_SITES.VENDOR_SITE_ID(+)) 
	and AHA.RELEASE_LOOKUP_CODE is NULL 
	and (AP.AP_INVOICE_PAYMENTS_ALL.REVERSAL_FLAG = 'N' or AP.AP_INVOICE_PAYMENTS_ALL.REVERSAL_FLAG is Null) 
	and (NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) &lt;&gt; 0 
	and INV_SOB.NAME in ('United States') 
	and AP.AP_INVOICES_ALL.INVOICE_NUM not Like 'IE%' 
	and AP.AP_INVOICES_ALL.PAYMENT_METHOD_CODE in ('EFT', 'WIRE') 
	and AP.AP_INVOICES_ALL.PAYMENT_STATUS_FLAG in ('N', 'P') 
	and (AP.AP_PAYMENT_SCHEDULES_ALL.DUE_DATE &lt;= SysDate + 45))</File>
