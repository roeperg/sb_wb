          <File RecordLimit="" FileFormat="17">aka:5fda36ca022c061b0852e8f1|||select AP_INV_VENDORS.VENDOR_NAME "AP Supplier Name",
	AP_INV_VENDORS.SEGMENT1 "AP Supplier Number",
	AP.AP_INVOICES_ALL.INVOICE_NUM "AP Invoice Hdr Num",
	AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE "AP Invoice Hdr Curr Code",
	NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) "AP Invoice Hdr Amt",
	AP_INV_VENDORS.VENDOR_TYPE_LOOKUP_CODE "AP Invoice Hdr Type",
	case 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '82' then 'United States' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '142' then 'United Kingdom' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '113' then 'Singapore' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID in ('129', '132', '145') then 'India' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '139' then 'Canada' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '186' then 'Australia' 
			else HR_ALL_ORGANIZATION_UNITS2.NAME 
		end as "AP Invoice Hdr Operating Unit",
	AP.AP_INVOICES_ALL.DESCRIPTION "AP Invoice Hdr Desc",
	AP.AP_INVOICES_ALL.INVOICE_DATE "AP Invoice Hdr Date",
--	Lower(
--		case 
--				when AP_VENDOR_SITES.REMITTANCE_EMAIL is NULL then SubStr(AP_INV_VENDORS.VENDOR_NAME, -(Length(AP_INV_VENDORS.VENDOR_NAME) - Instr(AP_INV_VENDORS.VENDOR_NAME, ',') - 1)) || '_' || SubStr(AP_INV_VENDORS.VENDOR_NAME, 1, Instr(AP_INV_VENDORS.VENDOR_NAME, ',') - 1) || '@intuit.com' 
--				else AP_VENDOR_SITES.REMITTANCE_EMAIL 
--			end) as "Remittance Email",
	lower(PER_ALL_PEOPLE.EMAIL_ADDRESS) as "REMITTANCE_EMAIL",
	NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) - NVL(AP.AP_INVOICES_ALL.AMOUNT_PAID, 0) "AP Invoice Hdr Balance",
	AP_INV_VENDORS.END_DATE_ACTIVE "AP Supplier Inactive Date",
	PER_ALL_PEOPLE.EMPLOYEE_NUMBER "Employee ID",
	case 
			when AP_INV_VENDORS.END_DATE_ACTIVE is NULL then 'Y' 
			else 'N' 
		end as "Active(Y/N)"
from AP.AP_SUPPLIERS AP_INV_VENDORS,
HR.PER_ALL_PEOPLE_F PER_ALL_PEOPLE,
 AP.AP_INVOICES_ALL, 
 HR.HR_ALL_ORGANIZATION_UNITS HR_ALL_ORGANIZATION_UNITS2, 
 AP.AP_SUPPLIER_SITES_ALL AP_VENDOR_SITES, 
	(
	select AP_INV_VENDORS.SEGMENT1,
		AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE,
		Sum(NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) - NVL(AP.AP_INVOICES_ALL.AMOUNT_PAID, 0))
        from AP.AP_SUPPLIERS AP_INV_VENDORS, AP.AP_INVOICES_ALL, HR.HR_ALL_ORGANIZATION_UNITS HR_ALL_ORGANIZATION_UNITS2, AP.AP_SUPPLIER_SITES_ALL AP_VENDOR_SITES
	where (AP.AP_INVOICES_ALL.VENDOR_ID = AP_INV_VENDORS.VENDOR_ID)
		and (AP.AP_INVOICES_ALL.VENDOR_SITE_ID = AP_VENDOR_SITES.VENDOR_SITE_ID(+)) 
		and (AP.AP_INVOICES_ALL.ORG_ID = HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID) 
		and AP_INV_VENDORS.VENDOR_TYPE_LOOKUP_CODE in ('EMPLOYEE') 
		and NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) - NVL(AP.AP_INVOICES_ALL.AMOUNT_PAID, 0) &lt;&gt; 0 
	group by AP_INV_VENDORS.SEGMENT1, AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE 
	having Sum(NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) - NVL(AP.AP_INVOICES_ALL.AMOUNT_PAID, 0)) &lt;= -1
	) aa 
where (AP.AP_INVOICES_ALL.VENDOR_ID = AP_INV_VENDORS.VENDOR_ID) 
	and (AP.AP_INVOICES_ALL.VENDOR_SITE_ID = AP_VENDOR_SITES.VENDOR_SITE_ID(+)) 
	and (AP.AP_INVOICES_ALL.ORG_ID = HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID)
	and TO_CHAR(AP_INV_VENDORS.EMPLOYEE_ID) = PER_ALL_PEOPLE.PERSON_ID
	and AP_INV_VENDORS.VENDOR_TYPE_LOOKUP_CODE in ('EMPLOYEE') 
	and NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) - NVL(AP.AP_INVOICES_ALL.AMOUNT_PAID, 0) &lt;&gt; 0 
	and aa.SEGMENT1 || aa.INVOICE_CURRENCY_CODE = AP_INV_VENDORS.SEGMENT1 || AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE
      AND  ( sysdate BETWEEN PER_ALL_PEOPLE.EFFECTIVE_START_DATE AND PER_ALL_PEOPLE.EFFECTIVE_END_DATE  )
    --and AP_INV_VENDORS.END_DATE_ACTIVE is not null
    and AP_INV_VENDORS.END_DATE_ACTIVE is  null
group by
AP_INV_VENDORS.VENDOR_NAME,
	AP_INV_VENDORS.SEGMENT1,
	AP.AP_INVOICES_ALL.INVOICE_NUM,
	AP.AP_INVOICES_ALL.INVOICE_CURRENCY_CODE,
	NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT),
	AP_INV_VENDORS.VENDOR_TYPE_LOOKUP_CODE,
	case 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '82' then 'United States' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '142' then 'United Kingdom' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '113' then 'Singapore' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID in ('129', '132', '145') then 'India' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '139' then 'Canada' 
			when HR_ALL_ORGANIZATION_UNITS2.ORGANIZATION_ID = '186' then 'Australia' 
			else HR_ALL_ORGANIZATION_UNITS2.NAME 
		end,
	AP.AP_INVOICES_ALL.DESCRIPTION,
	AP.AP_INVOICES_ALL.INVOICE_DATE,
--	Lower(
--		case 
--				when AP_VENDOR_SITES.REMITTANCE_EMAIL is NULL then SubStr(AP_INV_VENDORS.VENDOR_NAME, -(Length(AP_INV_VENDORS.VENDOR_NAME) - Instr(AP_INV_VENDORS.VENDOR_NAME, ',') - 1)) || '_' || SubStr(AP_INV_VENDORS.VENDOR_NAME, 1, Instr(AP_INV_VENDORS.VENDOR_NAME, ',') - 1) || '@intuit.com' 
--				else AP_VENDOR_SITES.REMITTANCE_EMAIL 
--			end),
	lower(PER_ALL_PEOPLE.EMAIL_ADDRESS),
	NVL(AP.AP_INVOICES_ALL.INVOICE_AMOUNT, AP.AP_INVOICES_ALL.BASE_AMOUNT) - NVL(AP.AP_INVOICES_ALL.AMOUNT_PAID, 0),
	AP_INV_VENDORS.END_DATE_ACTIVE,
	PER_ALL_PEOPLE.EMPLOYEE_NUMBER,
	case 
			when AP_INV_VENDORS.END_DATE_ACTIVE is NULL then 'Y' 
			else 'N' 
		end</File>
