          <File RecordLimit="" FileFormat="17">aka:5fda36ca022c061b0852e8f1|||SELECT
  PO.PO_REQUISITION_HEADERS_ALL.SEGMENT1 "REQ_HDR_NUM",
  PO.PO_REQUISITION_LINES_ALL.UNIT_PRICE*PO.PO_REQ_DISTRIBUTIONS_ALL.REQ_LINE_QUANTITY "REQ_LINE_AMOUNT",
  PO.PO_REQUISITION_LINES_ALL.LINE_NUM "REQ_LINE_NUM",
  case when (PO.PO_REQUISITION_LINES_ALL.VENDOR_ID = 1378 or PO.PO_REQUISITION_LINES_ALL.VENDOR_ID= 1418) then ' ' else PO.PO_REQUISITION_LINES_ALL.ITEM_DESCRIPTION END "REQ_LINE_ITEM_DESC",
  UPPER(REQUESTOR.FULL_NAME) "REQ_LINE_REQUESTOR",
  REQ_LINE_INV_MTL_CATEGORIES_B.SEGMENT1 || '.' || REQ_LINE_INV_MTL_CATEGORIES_B.SEGMENT2 "REQ_LINE_ITEM_CATEGORY",
  PO.PO_REQUISITION_LINES_ALL.NOTE_TO_AGENT "REQ_LINE_NOTE_TO_BUYER",
  PO.PO_REQUISITION_LINES_ALL.NEW_SUPPLIER_FLAG "REQ_LINE_NEW_SUPP_FLAG",
  PO.PO_REQUISITION_LINES_ALL.SUGGESTED_VENDOR_NAME "SUGG_SUPP_NAME",
  PO.PO_REQUISITION_HEADERS_ALL.CREATION_DATE "REQ_HDR_CRT_DATE",
  max(PO.PO_ACTION_HISTORY.ACTION_DATE) "REQ_HDR_APPROVE_DATE",
  PO.PO_REQUISITION_HEADERS_ALL.AUTHORIZATION_STATUS "REQ_HDR_AUTH_STATUS",
  PO.PO_REQUISITION_LINES_ALL.SUGGESTED_VENDOR_PHONE "REQ_LINE_SUPP_PHONE",
  REQUESTOR.EMAIL_ADDRESS "REQ_LINE_SUPP_PHONE",
  Preparer.FULL_NAME "REQ_HDR_PREPARER",
  Preparer.EMAIL_ADDRESS "REQ_HDR_PREP_EMAIL",
  PO.PO_REQUISITION_LINES_ALL.CREATION_DATE "REQ_LINE_CRT_DATE",
  trunc(PO.PO_REQUISITION_LINES_ALL.NEED_BY_DATE) "REQ_LINE_NEED_DATE",
  GL_COMB_REQ.SEGMENT1 "REQ_COMP_CD",
  GL_COMB_REQ.SEGMENT2 "REQ_GROUP_CD",
  GL_COMB_REQ.SEGMENT3 "REQ_DEPT_CD",
  GL_COMB_REQ.SEGMENT4 "REQ_ACCT_CD",
  GL_COMB_REQ.SEGMENT5 "REQ_INTC_CD",
  PO.PO_REQUISITION_LINES_ALL.CURRENCY_CODE "REQ_LINE_CURR_CODE",
  PO_REQ_LINE_TYPES_TL.LINE_TYPE "REQ_LINE_TYPE",
  PO.PO_ACTION_HISTORY.NOTE "REQ_HDR_APPROVAL_NOTE"
FROM
  PO.PO_REQUISITION_HEADERS_ALL,
  PO.PO_REQ_DISTRIBUTIONS_ALL,
  PO.PO_REQUISITION_LINES_ALL,
  HR.PER_ALL_PEOPLE_F  REQUESTOR,
  INV.MTL_CATEGORIES_B  REQ_LINE_INV_MTL_CATEGORIES_B,
  PO.PO_ACTION_HISTORY,
  HR.PER_ALL_PEOPLE_F  Preparer,
  GL.GL_CODE_COMBINATIONS  GL_COMB_REQ,
  PO.PO_LINE_TYPES_TL  PO_REQ_LINE_TYPES_TL,
  ( 
  select object_id, 
        max(sequence_num) AS max_seq
        from po.po_action_history
        group by object_id
  )  REQ_1NOTE,
  INV.MTL_CATEGORIES_TL  REQ_LINE_INV_MTL_CATEGORIES_TL
WHERE
  ( (GL_COMB_REQ.CHART_OF_ACCOUNTS_ID = 101 and GL_COMB_REQ.TEMPLATE_ID IS NULL) OR GL_COMB_REQ.CHART_OF_ACCOUNTS_ID IS NULL  )
  AND  ( GL_COMB_REQ.CODE_COMBINATION_ID(+)=PO.PO_REQ_DISTRIBUTIONS_ALL.CODE_COMBINATION_ID  )
  AND  ( PO.PO_REQUISITION_HEADERS_ALL.REQUISITION_HEADER_ID(+)=PO.PO_REQUISITION_LINES_ALL.REQUISITION_HEADER_ID  )
  AND  ( PO.PO_REQ_DISTRIBUTIONS_ALL.GL_CANCELLED_DATE IS NULL  )
  AND  ( PO.PO_REQUISITION_HEADERS_ALL.ENABLED_FLAG = 'Y' OR PO.PO_REQUISITION_HEADERS_ALL.ENABLED_FLAG IS NULL  )
  AND  ( PO.PO_REQUISITION_LINES_ALL.REQUISITION_LINE_ID(+)=PO.PO_REQ_DISTRIBUTIONS_ALL.REQUISITION_LINE_ID  )
  AND  ( Preparer.PERSON_ID(+)=PO.PO_REQUISITION_HEADERS_ALL.PREPARER_ID  )
  AND  ( REQUESTOR.PERSON_ID(+)=PO.PO_REQUISITION_LINES_ALL.TO_PERSON_ID  )
  AND  ( PO.PO_ACTION_HISTORY.OBJECT_ID=PO.PO_REQUISITION_HEADERS_ALL.REQUISITION_HEADER_ID  )
  AND  ( sysdate BETWEEN Preparer.EFFECTIVE_START_DATE AND Preparer.EFFECTIVE_END_DATE  )
  AND  ( ( sysdate BETWEEN REQUESTOR.EFFECTIVE_START_DATE AND REQUESTOR.EFFECTIVE_END_DATE  )
OR   ( REQUESTOR.EFFECTIVE_START_DATE is null)  )
  AND  ( REQ_LINE_INV_MTL_CATEGORIES_TL.CATEGORY_ID(+)=PO.PO_REQUISITION_LINES_ALL.CATEGORY_ID  )
  AND  ( PO_REQ_LINE_TYPES_TL.LINE_TYPE_ID(+)=PO.PO_REQUISITION_LINES_ALL.LINE_TYPE_ID  )
  AND  ( REQ_LINE_INV_MTL_CATEGORIES_B.STRUCTURE_ID=201  )
  AND  ( REQ_LINE_INV_MTL_CATEGORIES_B.CATEGORY_ID=REQ_LINE_INV_MTL_CATEGORIES_TL.CATEGORY_ID  )
  AND  ( REQ_1NOTE.OBJECT_ID=PO.PO_ACTION_HISTORY.OBJECT_ID  )
  AND  ( REQ_1NOTE.MAX_SEQ=PO.PO_ACTION_HISTORY.SEQUENCE_NUM  )
  AND  
  (
   PO.PO_REQUISITION_HEADERS_ALL.AUTHORIZATION_STATUS  =  'APPROVED'
   AND
   (
    PO.PO_REQUISITION_LINES_ALL.CANCEL_FLAG  =  'N'
    OR
    PO.PO_REQUISITION_LINES_ALL.CANCEL_FLAG  Is Null  
   )
   AND
   ( PO.PO_REQUISITION_LINES_ALL.LINE_LOCATION_ID IS NULL  )
   AND
   REQ_LINE_INV_MTL_CATEGORIES_B.SEGMENT1 || '.' || REQ_LINE_INV_MTL_CATEGORIES_B.SEGMENT2  NOT LIKE  '%Internal%'
   AND
   UPPER(REQUESTOR.FULL_NAME)  &lt;&gt;  'SCHEDULER, INTUIT'
   AND
   ( PO.PO_ACTION_HISTORY.ACTION_DATE BETWEEN sysdate-1 and sysdate  )
  )
GROUP BY
  PO.PO_REQUISITION_HEADERS_ALL.SEGMENT1, 
  PO.PO_REQUISITION_LINES_ALL.UNIT_PRICE*PO.PO_REQ_DISTRIBUTIONS_ALL.REQ_LINE_QUANTITY, 
  PO.PO_REQUISITION_LINES_ALL.LINE_NUM, 
  case when (PO.PO_REQUISITION_LINES_ALL.VENDOR_ID = 1378 or PO.PO_REQUISITION_LINES_ALL.VENDOR_ID= 1418) then ' ' else PO.PO_REQUISITION_LINES_ALL.ITEM_DESCRIPTION END, 
  UPPER(REQUESTOR.FULL_NAME), 
  REQ_LINE_INV_MTL_CATEGORIES_B.SEGMENT1 || '.' || REQ_LINE_INV_MTL_CATEGORIES_B.SEGMENT2, 
  PO.PO_REQUISITION_LINES_ALL.NOTE_TO_AGENT, 
  PO.PO_REQUISITION_LINES_ALL.NEW_SUPPLIER_FLAG, 
  PO.PO_REQUISITION_LINES_ALL.SUGGESTED_VENDOR_NAME, 
  PO.PO_REQUISITION_HEADERS_ALL.CREATION_DATE, 
  PO.PO_REQUISITION_HEADERS_ALL.AUTHORIZATION_STATUS, 
  PO.PO_REQUISITION_LINES_ALL.SUGGESTED_VENDOR_PHONE, 
  REQUESTOR.EMAIL_ADDRESS, 
  Preparer.FULL_NAME, 
  Preparer.EMAIL_ADDRESS, 
  PO.PO_REQUISITION_LINES_ALL.CREATION_DATE, 
  trunc(PO.PO_REQUISITION_LINES_ALL.NEED_BY_DATE), 
  GL_COMB_REQ.SEGMENT1, 
  GL_COMB_REQ.SEGMENT2, 
  GL_COMB_REQ.SEGMENT3, 
  GL_COMB_REQ.SEGMENT4, 
  GL_COMB_REQ.SEGMENT5, 
  PO.PO_REQUISITION_LINES_ALL.CURRENCY_CODE, 
  PO_REQ_LINE_TYPES_TL.LINE_TYPE, 
  PO.PO_ACTION_HISTORY.NOTE</File>
